{
  "name": "@zephraph/procedure",
  "version": "0.0.1",
  "description": "A library for simplifying verbose procedural workflows",
  "author": {
    "email": "zephraph@gmail.com",
    "name": "Justin Bennett",
    "url": "https://just-be.dev"
  },
  "scripts": {
    "build": "yarn build:cjs && yarn build:esm",
    "build:base": "esbuild src/index.ts --bundle --outdir=dist",
    "build:cjs": "yarn build:base --format=cjs",
    "build:esm": "yarn build:base --format=esm --out-extension:.js=.esm.js",
    "postbuild": "tsc",
    "dev": "esbuild src/index.ts --bundle --watch",
    "test": "jest test.ts"
  },
  "devDependencies": {
    "@types/babel__code-frame": "^7.0.2",
    "@types/jest": "^26.0.23",
    "esbuild": "^0.11.12",
    "esbuild-jest": "^0.5.0",
    "esbuild-register": "^2.5.0",
    "jest": "^26.6.3",
    "prettier": "^2.2.1",
    "typescript": "^4.2.4"
  },
  "dependencies": {
    "stacktracey": "^2.1.7",
    "ts-union-tools": "^0.0.1"
  },
  "license": "MIT"
}
